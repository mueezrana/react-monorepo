{
    "name": "my-yarn-workspaces-project",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "start": "concurrently \"yarn serve\" \"yarn json-server\"",
        "serve": "webpack serve --config webpack.config.js",
        "json-server": "json-server --watch db.json --port 5000"
    },
    "dependencies": {
