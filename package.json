        "@babel/preset-env": "^7.24.6",
        "@babel/preset-react": "^7.24.6",
        "axios": "^1.7.2",
        "babel-loader": "^9.1.3",
        "html-webpack-plugin": "^5.6.0",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "json-server": "^1.0.0-beta.1"
    }
}
